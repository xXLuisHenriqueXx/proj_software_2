{
  "name": "re.brinque",
  "version": "1.0.0",
  "main": "src/app.ts",
  "type": "commonjs",
  "scripts": {
    "dev": "npx prisma generate && tsx watch --env-file=.env.dev src/app.ts",
    "start": "npx prisma generate && tsx --env-file=.env src/app.ts",
    "build": "npx prisma generate && tsup src/app.ts --format cjs,esm --dts --out-dir dist"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "dotenv": "^17.2.1",
    "fastify": "^5.5.0",
    "fastify-type-provider-zod": "^5.0.3",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "prisma": "^6.14.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
